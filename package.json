{
  "name": "spendsentinel",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9002 --experimental-https",
    "genkit": "genkit start npm run dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "update-version": "node scripts/update-version.js",
    "version:major": "npm version major && npm run update-version",
    "version:minor": "npm version minor && npm run update-version",
    "version:patch": "npm version patch && npm run update-version",
    "deploy:with-cleanup": "echo 'Setting cleanup flag...' && NEXT_PUBLIC_FORCE_CLEANUP=true npm run build",
    "deploy:major": "npm run version:major && npm run build",
    "deploy:minor": "npm run version:minor && npm run build",
    "prerelease": "npm run typecheck && npm run lint && npm run build && npm run test:e2e:quick",
    "release": "npm run prerelease && npm run version:minor && npm run build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:quick": "playwright test tests/e2e/specs/critical-flows/complete-onboarding.spec.ts",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "i18n:check-unused": "node scripts/find-unused-translations.js"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@genkit-ai/googleai": "^1.9.0",
    "@genkit-ai/next": "^1.9.0",
    "@hookform/resolvers": "^4.1.3",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.3.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-query": "^5.66.0",
    "@types/mdx": "^2.0.13",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "genkit": "^1.9.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.475.0",
    "negotiator": "^1.0.0",
    "next": "^15.5.6",
    "next-mdx-remote": "^5.0.0",
    "patch-package": "^8.0.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "9.28.0",
    "eslint-config-next": "15.3.3",
    "genkit-cli": "^1.9.0",
    "glob": "^11.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
